      "invert": {
        "label": "Invert",
        "description": "Inverts black to white and vise versa.",
        "default_value": false,
        "type": "bool"
      },
      "contrast": {
        "label": "Contrast",
        "description": "The difference between the lightest color and the darkest color.",
        "default_value": 50,
        "type": "int",
        "minimum_value": 1,
        "maximum_value": 100
      },
      "brightness": {
        "label": "Brightness",
        "description": "The engraved picture is brighter when this value is bigger.",
        "default_value": 50,
        "type": "int",
        "minimum_value": 0,
        "maximum_value": 100
      },
      "white_clip": {
        "label": "White Clip",
